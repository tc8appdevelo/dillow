<!DOCTYPE html>
<html>
  <head>
    <title>Dillow</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{Rails.application.credentials.google_api_key}" %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>

    <script src="https://kit.fontawesome.com/bb8fa88233.js" crossorigin="anonymous"></script>
    <%= favicon_link_tag asset_path('favicon.png') %>

  </head>

  <body>
    <script type="text/javascript">
      window.alpineURL = "<%= image_url('alpine.png') %>";
      window.oceanCityBanner = "<%= image_url('ocean-city-banner.png') %>"
      window.savedHeartUrl = "<%= image_url('heart_filled.png') %>";
      window.notSavedHeartUrl = "<%= image_url('heart_outline.png') %>";
      window.trashUrl = "<%= image_url('trashcan.png') %>";
      window.editPencilUrl = "<%= image_url('editpencil2.png') %>";
      
    </script>



    <%= yield %>
  </body>
</html>
